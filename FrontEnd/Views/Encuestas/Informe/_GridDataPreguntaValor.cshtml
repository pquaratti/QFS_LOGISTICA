@model IEnumerable<Entidades.Vistas.PreguntaCantidad>

<div class="card card-hover card-dark mt-4 shadow-sm">
    <div class="card-header badge-@ViewBag.Badge">
        <div class="card-title mg-b-0 bd-transparent ">
            <h5>@ViewBag.Titulo</h5>
        </div>
    </div>
        
    <div class="card-body ">
        <div class="table-responsive ">
            <table class="table table-striped table-hover mg-b-0 ">
                <thead>
                    <tr>
                        <th scope="col">Pregunta</th>
                        <th scope="col">@ViewBag.Campo</th>
                    </tr>
                </thead>
                <tbody >
                    @foreach (Entidades.Vistas.PreguntaCantidad renglon in Model)
                    {
                        <tr style="">
                            <td>@renglon.Pregunta.encpreg_contenido</td>
                            <td>@renglon.Cantidad</td>
                        </tr>
                    }
                </tbody>
            </table>
        </div>  
    </div>
</div>

<script>
</script>
