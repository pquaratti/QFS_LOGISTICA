@{

    Entidades.App.Token oToken = Filters.VerificarToken.ConsultarToken();
}

<div class="content-header header-title-page ml-0 mr-0">
    <div class="col-md-7">
        <h4 class="content-title content-title-xs">Bienvenido @oToken.UserName !</h4>
        <p>@ViewBag.Subtitle</p>
    </div>
    <div class="col-md-5 pr-0">
        <div class="d-flex flex-row-reverse">

        </div>
    </div>
</div>

<div id="viewGridStatusProyectosVigentes"></div>

<script>

    function LoadDashboard()
    {
        _urlPartial = "@Url.RouteUrl("Default", new { action = "Proyectos_PartialGridStatus", controller = "StatisticsReports" })";
        APP_LoadPartialView('#viewGridStatusProyectosVigentes', {}, _urlPartial);
    }

</script>


